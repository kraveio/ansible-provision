# this will be the tfvars file on the provisioning server

aws_access_key = "{{ aws_access_key }}"
aws_secret_key = "{{ aws_secret_key }}"
aws_account_id = "{{ aws_account_id }}"
aws_region = "{{ aws_region }}"

vpc_id = "{{ provision_vpc_id }}"
vpc_name = "{{ provision_vpc_name }}"

aws_keypair_name = "{{ provision_keypair_name }}"
aws_private_key = "{{ provision_private_key }}"

state_bucket = "{{ provision_state_bucket }}"
state_file_vpc = "{{ provision_state_file }}"

state_file_security = "security.tfstate"
state_file_mind = "mind.tfstate"
state_file_frontend = "frontent.tfstate"

